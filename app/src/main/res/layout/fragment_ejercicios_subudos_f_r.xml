<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ejercicios_locales"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/negroclaro"
    tools:context=".EjerciciosLocalesFr">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_verEjerciciosSubidos_ejerciciosSubidosFr"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_actualizar_ejecicioSubidosFr2"
        android:layout_width="103dp"
        android:layout_height="101dp"
        android:contentDescription="Actualizar"
        android:scaleType="fitCenter"
        android:src="@drawable/actualziar"
        app:layout_constraintEnd_toEndOf="@+id/rv_verEjerciciosSubidos_ejerciciosSubidosFr"
        app:layout_constraintTop_toBottomOf="@+id/rv_verEjerciciosSubidos_ejerciciosSubidosFr" />

</androidx.constraintlayout.widget.ConstraintLayout>